      "name": "Pacientes",
      "item": [
        {
          "name": "Listar pacientes",
          "request": {"method": "GET", "url": "{{baseUrl}}/pacientes"}
        },
        {
          "name": "Buscar paciente por ID",
          "request": {"method": "GET", "url": "{{baseUrl}}/pacientes/1"}
        },
        {
          "name": "Criar paciente",
          "request": {
            "method": "POST",
            "header": [{"key": "Content-Type", "value": "application/json"}],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"nome\": \"João da Silva\",\n  \"cpf\": \"123.456.789-00\",\n  \"email\": \"joao@example.com\",\n  \"dataNascimento\": \"1990-05-20\"\n}"
            },
            "url": "{{baseUrl}}/pacientes"
          }
        },
        {
          "name": "Atualizar paciente",
          "request": {
            "method": "PUT",
            "header": [{"key": "Content-Type", "value": "application/json"}],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"nome\": \"João da Silva\",\n  \"cpf\": \"123.456.789-00\",\n  \"email\": \"joao.novo@example.com\",\n  \"dataNascimento\": \"1990-05-20\"\n}"
            },
            "url": "{{baseUrl}}/pacientes/1"
          }
        },
        {
          "name": "Remover paciente",
          "request": {"method": "DELETE", "url": "{{baseUrl}}/pacientes/1"}
        }
      ]
    },
